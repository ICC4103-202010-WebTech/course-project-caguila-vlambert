<div style="margin-left:360px ">
<div style="margin-right:360px ">
<%= form_with model: @user,url: sign_u_path, method: :patch do |form|  %>

  <ul class="list-group">
        <li class="list-group-item"><%= form.radio_button(:admin, "true") %>
        <%= label_tag(:admin, "Admin") %></li>
    
    <li class="list-group-item">
        <%= form.radio_button(:admin, "false") %>
        <%= label_tag(:admin, "No admin") %></li>
    <li class="list-group-item">
  <div class="form-group">
      <%= form.label :name %>
      <%= form.text_field :name, class: "form-control" %>
    </div></li>
    <li class="list-group-item">
     <div class="form-group">
      <%= form.label :location %>
      <%= form.text_field :location, class: "form-control" %>
    </div></li>
    <li class="list-group-item">
     <div class="form-group">
      <%= form.label :bio %>
      <%= form.text_field :bio, class: "form-control" %>
    </div></li>
    <li class="list-group-item">
       <div class="form-group">
      <%= form.label :email %>
      <%= form.text_field :email, class: "form-control" %>
    </div></li>
    <li class="list-group-item">
       <div class="form-group">
      <%= form.label :password %>
      <%= form.text_field :password, class: "form-control" %>
    </div></li>
  <li class="list-group-item">
  <div class="actions">
    <%= form.submit %>
  </div>
  </li>
  </ul>
<% end %>
</div>
</div>
