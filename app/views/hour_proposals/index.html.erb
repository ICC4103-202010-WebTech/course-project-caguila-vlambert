<p id="notice"><%= notice %></p>

<h1>Hour Proposals</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hour_proposals.each do |hour_proposal| %>
      <tr>
        <td><%= link_to 'Show', hour_proposal %></td>
        <td><%= link_to 'Edit', edit_hour_proposal_path(hour_proposal) %></td>
        <td><%= link_to 'Destroy', hour_proposal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hour Proposal', new_hour_proposal_path %>
